<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include("partials/head.ejs") %>
  </head>
  <body>
    <%- include(navbar) %>
    <!--Account Set Up-->
    <%if(flash["error"] || flash["success"]){ %>
    <div class="container invalidAlert py-4 text-center" id="hideMe">
      <div class="<%=flash['error'] ? 'alert alert-danger' : 'alert alert-success'%>" role="alert"><%=flash["error"] ? flash["error"] : flash["success"] && flash["success"]%></div>
    </div>
    <%}%>
    <section class="container-fluid pt-5">
      <div class="container-lg pt-3 mt-5">
        <div class="row account-layout-wrapper pt-3">
          <%- include("component/account/personal-info.ejs") %> <%-
          include("component/account/account-elements") %>
        </div>
      </div>
    </section>
    <%- include("partials/footer.ejs") %> <%- include("partials/scripts.ejs") %>
  </body>
</html>
